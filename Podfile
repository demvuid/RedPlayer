pod 'Google-Mobile-Ads-SDK'
pod 'Kingfisher'
# UI
pod 'MBProgressHUD'
pod 'IQKeyboardManagerSwift'
pod 'Reusable'
pod 'Eureka', '4.3.0'
# Database
pod 'RealmSwift'
# Rx
pod 'RxSwift'

  target 'BrowserVaultTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BrowserVaultUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
